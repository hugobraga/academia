  issn = {15708705},
  key = {Access control},
  keywords = {Boolean functions;Coloring;Computer networks;Electric load forecasting;Medium
	access control;Mobile telecommunication systems;Network protocols;Nuclear
	propulsion;Security systems;Sensor networks;Sensors;Telecommunication
	equipment;Telecommunication networks;Wireless sensor networks;Wireless
	telecommunication systems;},
