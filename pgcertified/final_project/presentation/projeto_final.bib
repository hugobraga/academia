}

@INPROCEEDINGS{20093812310584,
  author = {Decker, Eric B. and Rajendran, Venkatesh and Obraczka, Katia and
	Garcia-Luna-Aceves, J.J.},
  title = {The multi-channel flow-aware Medium Access Control protocol for wireless
	sensor networks},
  year = {2008},
  address = {Poznan, Poland},
  abstract = {We introduce the Multi-Channel Flow-Aware Medium Access Control protocol,
	or (MFLAMA), an energy-efficient, schedule-based, multi-channel medium-access
	control (MAC) protocol designed for data gathering applications in
	wireless sensor networks. MFLAMA improves the channel utilization
	by establishing collision-free transmission schedules across multiple
	channels. Energy efficiency is achieved by preventing packet collisions,
	idle listening, and transmissions to a node that is not ready to
	receive packets. We evaluate MFLAMA through extensive simulations
	and quantify the improvement in channel utilization through the use
	of multiple channels. Our results indicate that as we increase the
	number of orthogonal channels used for communication, there is significant
	improvement in channel utilization and queueing delay. However, we
	notice a "diminishing returns" effect as we increase the number of
	channels, i.e., the performance improvements observed decrease with
	the number of channels beyond a certain threshold. This threshold
	depends on the topology and traffic flow patterns being used. &copy;
	2008 IEEE.},
  copyright = {Compilation and indexing terms, Copyright 2010 Elsevier Inc.},
  booktitle = {IEEE International Symposium on Personal, Indoor and Mobile Radio
	Communications, PIMRC},
  key = {Medium access control},
  keywords = {Channel flow;Energy efficiency;Flow patterns;Network protocols;Radio
	communication;Security systems;Sensor networks;Telecommunication
	services;Traffic surveys;Transmissions;Wall flow;Wireless sensor
	networks;Wireless telecommunication systems;},
  language = {English},
